<div id="<%= dom_id newsarticle %>">
  <p>
    <strong>Source:</strong>
    <%= newsarticle.source %>
  </p>

  <p>
    <strong>Author:</strong>
    <%= newsarticle.author %>
  </p>

  <p>
    <strong>Title:</strong>
    <%= newsarticle.title %>
  </p>

  <p>
    <strong>Text:</strong>
    <%= newsarticle.text %>
  </p>

  <p>
    <strong>Imglink:</strong>
    <%= newsarticle.imglink %>
  </p>

  <p>
    <strong>Articlelink:</strong>
    <%= newsarticle.articlelink %>
  </p>

</div>
