<% @all_news['articles'].each do |art| %>

<div class="card">
    <div class="card-header">
        <%= art['source'['name']] %>
    </div>
  <img src=<%= art['urlToImage'] %> class="card-img-top" alt="...">
  <div class="card-body">
    <h5 class="card-title"><%= art['title'] %></h5>
    <p class="card-text"><%= art['description'] %></p>
    <a href=<%= art['url'] %> class="btn btn-primary">Read More</a>

  </div>
</div>

 <% end %>


